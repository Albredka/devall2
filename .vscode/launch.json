    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch localhost",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost/mypage.html",
            "webRoot": "${workspaceFolder}/wwwroot"
        },
        {
            "name": "Launch js",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceFolder}/chess/src/main/webapp/board.html"
        },
        {
            "type": "java",
            "name": "CodeLens (Launch) - AppTest",
            "request": "launch",
            "mainClass": "AppTest",
            "projectName": "chess"
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        
    ]
}