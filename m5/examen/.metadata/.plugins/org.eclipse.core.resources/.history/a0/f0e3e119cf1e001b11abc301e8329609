package examen;

import java.util.ArrayList;
import java.util.List;

public class Supermercado {
	private Almacen almacen;
	private List<Seccion> secciones;
	private List<Cesta> listaCestas;
	
	public Supermercado(Almacen almaceniones) {

		this.almacen = almacen;
		this.secciones = new ArrayList <Seccion>();
		this.listaCestas = new ArrayList <Cesta>();
	}
	
	public Supermercado(Almacen almacen, List<Seccion> secciones, List<Cesta> listaCestas) {

		this.almacen = almacen;
		this.secciones = secciones;
		this.listaCestas = listaCestas;
	}
	
	

}
